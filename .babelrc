{
  "presets": ["react-app"],
  "plugins": [
    [
      "module-resolver",
      {
        ".": ["./src"],
        "alias": {
          ".":"./src",
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@assets": "./src/assets",
          "@pages": "./src/pages",
          "@hooks": "./src/hooks",
          "myAlert": "./src/myAlert"
        }
      }
    ]
  ]
}
